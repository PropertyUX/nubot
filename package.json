{
  "name": "nubot",
  "version": "0.0.4",
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "author": "PropertyUX",
  "keywords": [
    "github",
    "nubot",
    "hubot",
    "chatbot",
    "bot",
    "campfire",
    "slack",
    "irc",
    "rocket.chat",
    "bot"
  ],
  "description": "A conversational context-aware chatbot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hubotio/hubot.git"
  },
  "dependencies": {
    "async": ">=0.1.0 <1.0.0",
    "chalk": "^1.0.0",
    "cline": "^0.8.2",
    "coffee-script": "^1.12.7",
    "connect-multiparty": "^1.2.5",
    "express": "^3.21.2",
    "hubot-playbook": "^1.1.0",
    "log": "1.4.0",
    "scoped-http-client": "0.11.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "~2.1.0",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "hubot-pretend": "^1.2.0",
    "mocha": "^2.1.0",
    "mockery": "^1.4.0",
    "nyc": "^11.0.2",
    "semantic-release": "^6.3.6",
    "sinon": "~1.17.0",
    "sinon-chai": "^2.8.0",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": "> 4.0.0",
    "npm": "> 2.0.0"
  },
  "main": "./index",
  "bin": {
    "nubot": "./bin/nubot"
  },
  "scripts": {
    "start": "bin/nubot",
    "debug": "node --debug start.js",
    "pretest": "standard",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:smoke": "node src/**/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
